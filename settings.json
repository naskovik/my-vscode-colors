
{
    "workbench.startupEditor": "newUntitledFile",
    //"editor.fontFamily": "JetBrainsMono NF",
    "editor.fontFamily": "FiraCode NF",
    //"editor.fontFamily": "Iosevka",
    "editor.fontSize": 16,
    "editor.lineHeight": 17,
    "editor.letterSpacing": 0.7,
    "files.trimTrailingWhitespace": true,
    "editor.fontWeight": "400",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 3,
    "editor.cursorBlinking": "solid",
    "editor.renderWhitespace": "trailing",
    "editor.tabSize": 4,
    "editor.tokenColorCustomizations": {
        "[Visual Gruvbox (Medium Dark)]": {
            "functions": "#8ec07c",
            "types": "#fabd2f",
            "keywords": "#f57878",
            "strings": "#b8bb26",
        },
        "[Gruvbox]": {
            "strings": "#b8bb26",//"#8ec07c",
            "variables": "#83a598",//"#fabd2f",
            "keywords": "#f57878",
            "functions": "#b8bb26",
            "types": "#fabd2f",
            "textMateRules": [
                {
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#f57878"
                    }
                },
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#fbf1c7"
                    }
                },
                {
                    "scope": "support.type.property-name",
                    "settings": {
                        "foreground": "#fbf1c7"
                    }
                }
            ]
        },
        "[Another Gruvbox Theme]": {
            "strings": "#8ec07c",
            "types": "#fabd2f",
            "keywords": "#f57878",
            "functions": "#b8bb26"
        },
        "[Gruvbox Dark Hard-flymin]": {
            "strings": "#8ec07c",
            "types": "#fabd2f",
            "keywords": "#f57878",
            "functions": "#b8bb26",
            "variables": "#83a598"
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "enabled": false,
    },
    "editor.guides.indentation": true,
    "editor.lineNumbers": "relative",
    "editor.fontLigatures": true,
    "editor.bracketPairColorization.enabled": true,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "editorLineNumber.activeForeground": "#fabd2f",
        "editor.background": "#282828",
        "editorBracketHighlight.foreground1": "#fabd2f",
        "editorBracketHighlight.foreground2": "#fabd2f",
        "editorBracketHighlight.foreground3": "#fabd2f",
        "editorBracketHighlight.foreground4": "#fabd2f",
        "editorBracketHighlight.foreground5": "#fabd2f",//"#fe8019",
        "sideBar.background": "#282828",
        "statusBar.background": "#1d2021",
        "tab.activeForeground": "#dcdcdc",
        "tab.inactiveForeground": "#dcdcdc",
        "sideBar.foreground": "#dcdcdc",
        "editorLineNumber.foreground": "#dcdcdc"
    },
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Gruvbox"
}
